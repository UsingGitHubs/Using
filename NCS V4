local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "NCS Hub V4", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"}) -- Orion
OrionLib:MakeNotification({
	Name = "Thanks!",
	Content = "Thank You For Using NCS V4", --Text
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel("Main")--Main

local Section = Tab:AddSection({ --Player
	Name = "Player"
})

Tab:AddSlider({
	Name = "Speed",
	Min = 16,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(s)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s -- speed
	end    
})

Tab:AddSlider({
	Name = "Jump Power",
	Min = 50,
	Max = 350,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump Power",
	Callback = function(s)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = s --jump Power
	end    
})

local Section = Tab:AddSection({
	Name = "Main"
})

local Tab = Window:MakeTab({
	Name = "Other Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel("These are not mine scripts") --other scripts

local Section = Tab:AddSection({
	Name = "Universal"
})

Tab:AddButton({
	Name = "Infinity Yield!",
	Callback = function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
  	end    
})

local Tab = Window:MakeTab({
	Name = "Settings",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Destroy Gui!",
	Callback = function()
		OrionLib:Destroy()
  	end    
})

local Tab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel("Gui: Orion ")

Tab:AddLabel("Script: Me ")

Tab:AddLabel("Other Scripts: I dont know all names ")

Tab:AddLabel("Update: 4")

Tab:AddButton({
	Name = "Join Discord!",
	Callback = function()
		setclipboard("https://discord.gg/wKK8hQ2PFG")
		OrionLib:MakeNotification({
	Name = "Discord",
	Content = "Discord Link Copied To Clipboard",
	Image = "rbxassetid://4483345998",
	Time = 5
})
  	end    
})  

OrionLib:Init()

